<script>
    $(document).ready(function () {
        var openedSections = [];

        if (window.name[0] === "|") {

            openedSections = JSON.parse(window.name.split("|")[1]);
        }

        console.log(window.name);

        openedSections.forEach(function (val) {

            $("#" + val.listId).addClass("in"); // open the list
            $("#" + val.arrowParentId).children('a > span.arrow').toggleClass('open');
        });

        $('.page-sidebar-menu').on('click', '.nav-link', function (e) {
            var arrowParentId = $(this).attr("id");
            var listId = $(this).parent().children('ul .aha-sub-menu').attr("id");
            //console.log($(this).parent());
            if ($(this).children('a > span.arrow').hasClass('open')) {
                console.log("open");
                for (var i = 0, os; os = openedSections[i]; i++) {
                    console.log("open1");
                    if (os.arrowParentId === arrowParentId) {
                        console.log("open2");
                        openedSections.splice(i, 1);
                        window.name = "|" + JSON.stringify(openedSections);
                        break;
                    }
                }

            } else {
                openedSections.push({ arrowParentId: arrowParentId, listId: listId });
                window.name = "|" + JSON.stringify(openedSections);

            }

            console.log(openedSections);

            $(this).children('a > span.arrow').toggleClass('open');


            
            //console.log($(this).parent().parent().parent().css('height'));

        });
        $('.page-content').css('height', $(".page-sidebar").css('height'));
    });
</script>

